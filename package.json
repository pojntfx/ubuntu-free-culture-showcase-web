{
  "name": "ubuntu-free-culture-showcase-frontend-web",
  "version": "0.0.1-alpha",
  "private": true,
  "dependencies": {
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "serve": "^6.5.8",
    "vanilla-framework": "^1.7.1",
    "vanilla-framework-react": "^0.1.3"
  },
  "scripts": {
    "dev": "npm-run-all -p watch-css start-dev",
    "start": "serve -s build",
    "start-dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o public/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o public/ --watch --recursive"
  },
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebookincubator/create-react-app).",
  "main": "index.js",
  "devDependencies": {
    "eslint-plugin-react": "^7.8.2",
    "prop-types": "^15.6.1",
    "styled-components": "^3.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pojntfx/ubuntu-free-culture-showcase-frontend-web.git"
  },
  "keywords": [
    "ubuntu",
    "culture",
    "wallpaper",
    "photography",
    "music",
    "video"
  ],
  "author": "Felicitas Pojtinger",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/pojntfx/ubuntu-free-culture-showcase-frontend-web/issues"
  },
  "homepage": "/"
}
